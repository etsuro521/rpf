<%= render 'shared/tab' %>
<div class="row">
        <div class="col-md-6 col-md-offset-3 woocommerce">
                <%= form_with(model: @team, local: true) do |f| %>
                        <%= render 'shared/error_messages', object: f.object %>

                        <%= f.label :name, class:"sr-only" %>
                        <div class="col-sm-10">
                                <%= f.text_field :name, class: 'form-control', placeholder:"Team Name" %>
                        </div>

                        <%= f.submit "Create", class: "btn btn-primary col-sm-2" %>
                <% end %>
        </div>
</div>
